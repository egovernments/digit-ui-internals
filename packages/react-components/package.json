{
  "name": "@egovernments/digit-ui-react-components",
  "version": "1.0.100",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.modern.js",
  "source": "src/index.js",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "example": "cd example && npm run start",
    "build": "microbundle-crl --no-compress --format modern,cjs",
    "start": "microbundle-crl watch --no-compress --format modern,cjs",
    "prepare": "yarn build",
    "predeploy": "cd example && yarn install && yarn run build",
    "deploy": "gh-pages -d example/build",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public -o dist-storybook",
    "deploy-storybook": "npm run build-storybook && surge --project dist-storybook --domain react-components-$npm_package_version.surge.sh"
  },
  "peerDependencies": {
    "react": "^17.0.0",
    "react-router-dom": "^5.2.0"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.1.10",
    "@storybook/addon-essentials": "^6.1.10",
    "@storybook/addon-links": "^6.1.10",
    "@storybook/node-logger": "^6.1.10",
    "@storybook/preset-create-react-app": "^3.1.5",
    "@storybook/react": "^6.1.10",
    "babel-eslint": "^10.0.3",
    "cross-env": "^7.0.3",
    "gh-pages": "^2.2.0",
    "husky": ">=4",
    "lint-staged": ">=10",
    "microbundle-crl": "^0.13.10",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^4.0.1"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "react-hook-form": "^6.7.0",
    "react-i18next": "^11.7.3",
    "react-table": "^7.6.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
